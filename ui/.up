#!/bin/bash

# This is equivalent of the local comment to make the env up and running


function delete_source_if_present() {
    if [ -f source.zip ]; then
        rm source.zip
    fi
}

delete_source_if_present
echo "Pre clean up"

git archive -o source.tar HEAD
echo "Packing"
aws s3 cp source.tar s3://constellation-prior/tmp/source.tar

delete_source_if_present
echo "Post clean up"
